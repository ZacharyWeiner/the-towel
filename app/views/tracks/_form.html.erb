<%= form_with(model: track, local: true) do |form| %>
  <% if track.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(track.errors.count, "error") %> prohibited this track from being saved:</h2>

      <ul>
      <% track.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :track_name %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <%= form.text_area :description, id: :track_description %>
  </div>

  <div class="field">
    <%= form.label :events_id %>
    <%= form.text_field :events_id, id: :track_events_id %>
  </div>

  <div class="field">
    <%= form.label :location_id %>
    <%= form.text_field :location_id, id: :track_location_id %>
  </div>

  <div class="field">
    <%= form.label :cohort_id %>
    <%= form.text_field :cohort_id, id: :track_cohort_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
