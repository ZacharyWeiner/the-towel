<div class="jumbotron jumbotron-fluid" >
  <div class="container" style='color:white'>
    <div class='row'>
      <div class='col-md-3'></div>
      <div class='col-md-6'>
      <h1>Photos</h1>
      <div class='col-md-3'></div>
    </div>
  </div>
  </div>
  </div>

<div class="container">
  <%= link_to 'Add A Picture', new_photo_path, class: 'text-right' %>
  <div class='row'>
    <% @photos.each do |photo| %>
      <div class='col-md-3'>
        <%= link_to photo_path(photo) do %>
        <%= image_tag photo.picture, style: 'width:100%; max-height:200px; object-fit: scale-down;' %>
        <p><%= photo.caption %></p>
        <% end %>
      </div>
    <% end %>
  </div>
</div>



