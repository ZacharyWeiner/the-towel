<p id="notice"><%= notice %></p>

<h1>Transits</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Departure time</th>
      <th>Title</th>
      <th>Travel type</th>
      <th>Company</th>
      <th>Route number</th>
      <th>Departure location</th>
      <th>Departure map</th>
      <th>Arrival location</th>
      <th>Arrival time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transits.each do |transit| %>
      <tr>
        <td><%= transit.date %></td>
        <td><%= transit.departure_time %></td>
        <td><%= transit.title %></td>
        <td><%= transit.travel_type %></td>
        <td><%= transit.company %></td>
        <td><%= transit.route_number %></td>
        <td><%= transit.departure_location %></td>
        <td><%= transit.departure_map %></td>
        <td><%= transit.arrival_location %></td>
        <td><%= transit.arrival_time %></td>
        <td><%= link_to 'Show', transit %></td>
        <td><%= link_to 'Edit', edit_transit_path(transit) %></td>
        <td><%= link_to 'Destroy', transit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transit', new_transit_path %>
