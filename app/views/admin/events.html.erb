
<div class='container'>
  <div class='row'>
    <div class='col-md-3'>
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">Events</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body table-responsive no-padding">
          <table class="table table-hover">
            <tbody><tr>
              <th>Title</th>
              <th>Manage</th>
            </tr>
            <% @event.cohort.events.each do |event| %>
              <tr>
                <td><%= event.title %></td>
                <td><%= link_to "Manage",  event_admin_path(event), class: 'btn btn-warning' %></td>
              </tr>
            <% end %>
          </tbody></table>
        </div>
        <!-- /.box-body -->
      </div>
    </div>
    <div class='col-md-9'>
      <h1><%= @event.title unless @event.nil? %></h1>
      <div class="row">
        <div class='col-md-6'>
          <%= render 'admin/partials/events/attendees'%>
        </div>
        <div class='col-md-6'>
          <%= render 'admin/partials/events/waitlist'%>
        </div>
      </div>
    </div>
  </div>
</div>




