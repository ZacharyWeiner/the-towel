<div class='row'>
  <div class='col-md-4'>
    <div class="box box-primary">
      <div class="box-header with-border">
        <h3 class="box-title"><%= @housing.name %></h3>
      </div>
      <!-- /.box-header -->
      <div class="box-body">
        <strong><i class="fa fa-book margin-r-5"></i> Street Address</strong>
        <p class="text-muted">
          <%= "#{@housing.street_number} #{@housing.street_name}" %>
        </p>
        <hr>
        <strong><i class="fa fa-map-marker margin-r-5"></i> City</strong>
        <p class="text-muted"><%= @housing.location.name %></p>
        <hr>
        <strong><i class="fa fa-pencil margin-r-5"></i> Features</strong>
        <p>
          <% @housing.tags.each do |tag|%>
            <span class="label label-primary"><%= tag.name %></span>
          <% end %>
        </p>
        <hr>
      </div>
    </div>
  </div>
  <div class='col-md-8'>
    <%= render 'admin/partials/housing/requested_roomates'%>
    <%= render 'admin/partials/housing/perfect_match'%>
    <%= render 'admin/partials/housing/pretty_close_matches'%>
    <%= render 'admin/partials/housing/poor_matches'%>
  </div>
</div>





