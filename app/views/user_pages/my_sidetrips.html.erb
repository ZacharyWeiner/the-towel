<div class="row">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header">
        <h3 class="box-title">Side Trips</h3>
      </div>
      <!-- /.box-header -->
      <div class="box-body table-responsive no-padding">
        <table class="table table-hover">
          <tbody><tr>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Title</th>
            <th>Description</th>
            <th>View</th>
          </tr>
          <% @sidetrips.each do |sidetrip|%>
            <tr>
              <td><%= sidetrip.start_date %></td>
              <td><%= sidetrip.end_date %></td>
              <td><%= sidetrip.title %></td>
              <td><%= sidetrip.description %></td>
              <td><%= link_to "View", side_trip_path(sidetrip), target: "_blank" %></td>
            </tr>
          <% end %>
        </tbody></table>
      </div>
      <!-- /.box-body -->
    </div>
    <!-- /.box -->
  </div>
</div>
